@model SpeechToTextProject.WebUI.Dtos.CreateAudioDto

@{
    Layout = null;
}


<link href="~/lib/bootstrap/dist/css/bootstrap.min.css" rel="stylesheet" />

<form method="post">
<div class="container">
    <br /><br />
    <label>Başlık</label>
    <input type="text" asp-for="Title" class="form-control"/>
    <br />
    <label>Ses Dosyası Fotoğraf</label>
    <input type="text" asp-for="ImageUrl" class="form-control"/>
    <br />
    <label>Ses Dosyası Yolu</label>
    <input type="text" asp-for="FilePath" class="form-control"/>
    <br />
    <button class="btn btn-primary">Ekle</button>
</div>
</form>